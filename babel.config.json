{
  "presets": [
    [
      "react-app",
      {
        "runtime": "automatic"
      }
    ]
  ],
  "plugins": [
    [
      "babel-plugin-transform-remove-console",
      {
        "exclude": ["error", "warn"]
      }
    ]
  ],
  "env": {
    "production": {
      "plugins": [
        [
          "babel-plugin-transform-remove-console",
          {
            "exclude": ["error"]
          }
        ]
      ]
    }
  }
}